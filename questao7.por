programa { // vetores - cálculo agregado
  funcao inicio() {
        inteiro vetor[7] = {22, 24, 20, 21, 25, 23, 19}
        real media, soma

        soma = vetor[0] + vetor[1] + vetor[2] + vetor[3] + vetor[4] + vetor[5] + vetor[6]

